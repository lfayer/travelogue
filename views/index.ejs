<style id="customized-content-colors">			a {
				color: #505050;
			}

			a:hover {
				color: #282828;
			}

			body, .block, .one-column, footer {
				background-color: #eaeaea;
				color: #505050;
			}

			blockquote {
				color: #505050;
			}

			blockquote > p:before, blockquote > p:after {
				color: #505050;
			}

			#intro {
				background: #dddddd;
				color: #505050;
			}

			.text-column img.size-medium {
				border-color: white;
			}

			.text-column > p strong {
				color: #202020;
			}

			#main-menu {
				background: #505050;
				color: #ffffff;
			}

			#main-menu:hover {
				background-color: #505050!important;
			}

			#main-menu dd, #main-menu dt {
				background: #03272d;
			}

			#main-menu .menu a.current {
				border-color: #03272d;
			}

			#main-menu .menu a:hover, #main-menu .menu a:hover > dt {
				background: #03272d;
			}

			#main-menu a {
				color: #ffffff;
			}

			.mejs-video .mejs-controls .mejs-time-rail .mejs-time-current {
				background: #03272d;
			}

			.mejs-video .mejs-controls .mejs-time-rail .mejs-time-loaded {
				background: #ffffff;
			}

			.sk-gallery  figcaption,
			.sk-video figcaption {
				background: rgba(3,39,45, 0.5);
				color: #ffffff;
			}

			.partage > a {
				color: #ffffff;
			}

			.scrolldown,
			.scrolldown:hover {
				color: #ffffff;
			}

			.scrolldown:after {
				border-color: #ffffff;
			}

			.post-edit-link {
				border-color: #505050;
				color: #505050;
				background-color: #eaeaea;
			}

			.post-edit-link:hover {
				color: #ffffff;
				background-color: #505050;
			}

			.page .menu .menu-item > a {
				border-color: #d0d0d0;
			}

			.wp-caption-text {
				color: #282828;
			}

